{
    "expo": {
        "name": "SellIt",
        "slug": "SellIt",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/logo.png",
        "splash": {
            "image": "./assets/images/Screensh.png",
            "resizeMode": "cover",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.otokinagreatness.sellit",
            "buildNumber": "1.0.0"
        },
        "android": {
            "package": "com.otokinagreatness.sellit",
            "versionCode": 1,
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/logo.png",
                "backgroundColor": "#FFFFFF"
            }

        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "description": "A mobile application where you sell things you're done with,\n",
        "plugins": [
            "sentry-expo"
        ]
    }
}